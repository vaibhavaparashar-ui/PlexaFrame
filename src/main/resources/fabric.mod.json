{
  "schemaVersion": 1,
  "id": "plexaframe",
  "version": "${version}",
  "name": "PlexaFrame",
  "description": "A client-side Fabric mod focused on improving rendering performance in Minecraft 1.21.1 and boosting frame rates.",
  "authors": [
    "Vaibhavparashar"
  ],
  "contact": {
    "homepage": "https://fabricmc.net/",
    "sources": "https://github.com/vaibhavaparashar-ui/PlexaFrame"
  },
  "license": "GNU LESSER GENERAL PUBLIC LICENSE",
  "icon": "assets/plexaframe/icon.png",
  "environment": "client",
  "entrypoints": {
    "main": [
      "com.vaibhavparashar.plexaframe.PlexaFrame"
    ],
  "client": [
    "com.vaibhavparashar.plexaframe.mixin.ChunkRendererRegionMixin",
    "com.vaibhavparashar.plexaframe.mixin.CameraUpdateMixin"
]
  },
  "mixins": [
    "plexaframe.mixins.json",
    "plexaframe.client.mixins.json"
  ],
  "depends": {
    "fabricloader": ">=0.15.0",
    "minecraft": ">=1.21.1",
    "java": ">=21",
    "fabric-api": "*"
  }
}